{
    "fileExtensions": [ ".less" ],
    "attributeQuotes": { "enabled": true, "style": "single" },
    "borderZero": { "enabled": true, "style": "zero" },
    "comment": { "enabled": false},
    "decimalZero": {"enabled": true, "style": "leading"},
    "depthLevel": {"enabled": false},
    "duplicateProperty": { "enabled": true, "exclude": ["background"]},
    "emptyRule": { "enabled": true},
    "finalNewline": { "enabled": false},
    "hexLength": { "enabled": false},
    "hexNotation": { "enabled": false},
    "hexValidation": { "enabled": true},
    "idSelector": { "enabled": true, "exclude": []},
    "importPath": { "enabled": true, "exclude": []},
    "importantRule": { "enabled": true},
    "maxCharPerLine": {"enabled": true, "limit": 110},
    "propertyOrdering": { "enabled": false},
    "propertyUnits": { "enabled": false},
    "qualifyingElement": { "enabled": true, "allowWithAttribute": true, "allowWithClass": true, "allowWithId": false},
    "singleLinePerProperty": { "enabled": true},
    "singleLinePerSelector": { "enabled": false},
    "spaceAfterComma": { "enabled": true, "style": "one_space" },
    "spaceAfterPropertyColon": { "enabled": true, "style": "one_space" },
    "spaceAfterPropertyName": { "enabled": true, "style": "no_space" },
    "spaceAfterPropertyValue": { "enabled": true, "style": "no_space" },
    "spaceBeforeBrace": { "enabled": true, "style": "one_space" },
    "spaceBeforeComma": { "enabled": true, "style": "no_space" },
    "spaceBetweenParens": { "enabled": true, "style": "no_space" },
    "stringQuotes": { "enabled": true, "style": "single" },
    "trailingSemicolon": { "enabled": true},
    "trailingWhitespace": { "enabled": true},
    "urlFormat": { "enabled": true, "style": "relative" },
    "urlQuotes": { "enabled": true},
    "zeroUnit": { "enabled": true, "style": "no_unit" }
}
