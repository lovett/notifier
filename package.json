{
  "name": "notifier",
  "private": true,
  "version": "0.0.0",
  "description": "A notification service for user-generated events",
  "main": "notifier.js",
  "scripts": {
    "build:worker": "webpack --config webpack-worker.js",
    "build:worker:dev": "webpack --config webpack-worker-dev.js",
    "build:app": "webpack --config webpack-app.js",
    "build:app:dev": "webpack --config webpack-app-dev.js",
    "build": "npm run build:app && npm run build:worker",
    "start": "node notifier.js",
    "devserver": "NODE_ENV=dev nodemon server",
    "hooks": "cp hooks/* .git/hooks/",
    "onemessage": "clients/send-notification -s $NOTIFIER_DEV -p 0 -t test -e '5 hours'",
    "retract": "clients/send-notification -c -l test",
    "test:server": "mocha --bail --reporter min test",
    "test:client": "karma start app/test/karma.conf.js --single-run",
    "coverage:server": "istanbul cover --dir coverage/server _mocha -- -R min test",
    "coverage:client": "echo 'Nope. Client coverage is automatically written by test:client'",
    "resetdb": "rm notifier.sqlite; touch notifier.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lovett/notifier.git"
  },
  "author": "Bill Lovett",
  "homepage": "https://github.com/lovett/notifier",
  "dependencies": {
    "angular-cookies": "1.6.5",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "cookie-parser": "1.4.3",
    "dateparser": "1.0.6",
    "ejs": "2.5.7",
    "express": "4.15.3",
    "morgan": "1.8.2",
    "nconf": "0.8.4",
    "needle": "1.6.0",
    "passport": "0.3.2",
    "passport-cookie": "1.0.4",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "response-time": "2.3.2",
    "sanitize-html": "1.14.1",
    "sequelize": "4.4.2",
    "serve-favicon": "2.4.3",
    "sqlite3": "3.1.8",
    "useragent": "2.2.1",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@types/angular": "1.6.28",
    "@types/body-parser": "1.16.4",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "1.4.0",
    "@types/express": "4.0.36",
    "@types/fastclick": "1.0.28",
    "@types/nconf": "0.0.34",
    "@types/needle": "1.4.0",
    "@types/node": "8.0.19",
    "@types/passport": "0.3.3",
    "@types/passport-http": "0.3.2",
    "@types/passport-local": "1.0.30",
    "@types/response-time": "2.3.0",
    "@types/sanitize-html": "1.13.31",
    "@types/serve-favicon": "2.2.28",
    "@types/useragent": "2.1.1",
    "@types/uuid": "3.4.0",
    "angular": "1.6.5",
    "angular-animate": "1.6.5",
    "angular-mocks": "1.6.5",
    "angular-resource": "1.6.5",
    "angular-route": "1.6.5",
    "angular-sanitize": "1.6.5",
    "angular-touch": "1.6.5",
    "chai": "4.1.0",
    "chance": "1.0.10",
    "eslint": "4.3.0",
    "eslint-plugin-mocha": "4.11.0",
    "fastclick": "1.0.6",
    "grunt": "1.0.1",
    "grunt-angular-templates": "1.1.0",
    "grunt-autoprefixer": "3.0.4",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-less": "1.4.1",
    "grunt-contrib-uglify": "3.0.1",
    "grunt-contrib-watch": "1.0.0",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.3",
    "karma": "1.7.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sinon": "1.0.5",
    "less": "2.7.2",
    "lesshint": "4.1.1",
    "mocha": "3.5.0",
    "ng-cache-loader": "0.0.25",
    "nodemon": "1.11.0",
    "normalize.css": "7.0.0",
    "sinon": "3.0.0",
    "supertest": "3.0.0",
    "ts-loader": "^2.3.2",
    "ts-node": "3.3.0",
    "tslint": "5.5.0",
    "typescript": "2.4.2",
    "webpack": "^3.5.4"
  }
}
