{
  "name": "notifier",
  "private": true,
  "version": "0.0.0",
  "description": "A notification service for user-generated events",
  "main": "notifier.js",
  "scripts": {
    "build:worker": "webpack --config webpack-worker.ts",
    "build:worker:dev": "NODE_ENV=dev webpack --config webpack-worker.ts",
    "build:app": "webpack --config webpack-app.js",
    "build:app:dev": "webpack --config webpack-app-dev.js",
    "build": "npm run build:app && npm run build:worker",
    "start": "node notifier.js",
    "devserver": "NODE_ENV=dev nodemon server",
    "hooks": "cp hooks/* .git/hooks/",
    "onemessage": "clients/send-notification -s $NOTIFIER_DEV -p 0 -t test -e '5 hours' -u 'http://example.com'",
    "retract": "clients/send-notification -c -l test",
    "test:server": "mocha --bail --reporter min test",
    "test:client": "karma start app/test/karma.conf.js --single-run",
    "coverage:server": "istanbul cover --dir coverage/server _mocha -- -R min test",
    "coverage:client": "echo 'Nope. Client coverage is automatically written by test:client'",
    "resetdb": "rm notifier.sqlite; touch notifier.js",
    "watch": "livereload public -p 35740 -d -u 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lovett/notifier.git"
  },
  "author": "Bill Lovett",
  "homepage": "https://github.com/lovett/notifier",
  "dependencies": {
    "angular-cookies": "1.6.6",
    "body-parser": "1.18.1",
    "compression": "1.7.0",
    "cookie-parser": "1.4.3",
    "dateparser": "1.0.6",
    "ejs": "2.5.7",
    "express": "4.15.4",
    "glob": "7.1.2",
    "morgan": "1.8.2",
    "nconf": "0.8.4",
    "needle": "2.0.1",
    "passport": "0.4.0",
    "passport-cookie": "1.0.4",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "response-time": "2.3.2",
    "sanitize-html": "1.14.1",
    "sequelize": "4.10.0",
    "serve-favicon": "2.4.4",
    "sqlite3": "3.1.11",
    "useragent": "2.2.1",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@types/angular": "1.6.32",
    "@types/body-parser": "1.16.5",
    "@types/compression": "0.0.34",
    "@types/cookie-parser": "1.4.1",
    "@types/es6-promise": "0.0.33",
    "@types/express": "4.0.37",
    "@types/glob": "5.0.32",
    "@types/nconf": "0.0.34",
    "@types/needle": "2.0.2",
    "@types/node": "8.0.29",
    "@types/passport": "0.3.4",
    "@types/passport-http": "0.3.2",
    "@types/passport-local": "1.0.31",
    "@types/response-time": "2.3.1",
    "@types/sanitize-html": "1.13.31",
    "@types/serve-favicon": "2.2.29",
    "@types/useragent": "2.1.1",
    "@types/uuid": "3.4.2",
    "@types/webpack": "3.0.11",
    "angular": "1.6.6",
    "angular-mocks": "1.6.6",
    "angular-resource": "1.6.6",
    "angular-route": "1.6.6",
    "angular-sanitize": "1.6.6",
    "angular-touch": "1.6.6",
    "chai": "4.1.2",
    "chance": "1.0.11",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.7",
    "eslint": "4.7.2",
    "eslint-plugin-mocha": "4.11.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.5",
    "karma": "1.7.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sinon": "1.0.5",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "lesshint": "4.1.2",
    "livereload": "0.6.2",
    "mocha": "3.5.3",
    "ng-cache-loader": "0.0.25",
    "nodemon": "1.12.1",
    "normalize.css": "7.0.0",
    "sinon": "3.3.0",
    "supertest": "3.0.0",
    "ts-loader": "2.3.7",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.5.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "webpack": "3.6.0"
  }
}
