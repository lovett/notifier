{
  "compilerOptions": {
    "alwaysStrict": true,
    "module": "commonJS",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noEmit": true,
    "target": "ES6",
    "downlevelIteration": true,
    "lib": [
      "es6"
    ]
  },
  "include": [
    "notifier.ts",
    "modules/**/*",
    "app/**/*.ts"
  ],
  "exclude": [
    "test/*",
    "app/scripts/worker.ts"
  ]
}
