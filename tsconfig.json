{
  "compilerOptions": {
    "alwaysStrict": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "ES5",
    "strict": true,
    "noImplicitAny": false,
  }
}
