{
  "compilerOptions": {
    "alwaysStrict": true,
    "downlevelIteration": true,
    "extendedDiagnostics": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "ES6",
    "strict": true,
    "noImplicitAny": true
  },
  "exclude": [
    "**/flycheck_*",
    "**/#*",
    "**/.*"
  ]

}
